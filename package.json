{
	"name": "flashcardz",
	"description": "Intelligently quizes you",
	"repository": "https://github.com/jamescostian/flashcardz",
	"version": "0.1.0",
	"license": "MIT",
	"contributors": [
		"James Costian <james@jamescostian.com> (http://jamescostian.com)"
	],
	"engines": {
		"node": "0.12"
	},
	"os": [
		"!win32"
	],
	"main": "lib/module.js",
	"scripts": {
		"lint": "jshint package.json lib test",
