		"commander": "2.8.x"
	},
	"devDependencies": {
		"mock-stdin": "0.3.x",
		"std-mocks": "1.0.x",
		"jshint": "2.8.x",
		"coveralls": "2.11.x",
		"tape": "4.0.x",
		"faucet": "0.0.1",
		"istanbul": "0.3.x"
	}
}
