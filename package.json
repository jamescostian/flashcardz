{
  "name": "flashcardz",
  "description": "Intelligently quizes you",
  "version": "0.2.2",
  "author": "James Costian <james@jamescostian.com>",
  "bin": {
    "flash": "bin/flash"
  },
  "bugs": {
    "url": "https://github.com/jamescostian/flashcardz/issues"
  },
  "dependencies": {
    "commander": "^2",
    "inquirer": "^2",
    "stream-string": "^1",
    "update-notifier": "^1"
  },
  "devDependencies": {
    "coveralls": "^2",
    "istanbul": "^0.4",
    "mock-stdin": "^0.3",
    "nyc": "^10",
    "snazzy": "^5",
    "std-mocks": "^1",
    "tap-spec": "^4",
    "tape": "^4"
  },
  "homepage": "https://github.com/jamescostian/flashcardz",
  "keywords": [
    "cards",
    "flashcards",
    "quiz"
  ],
  "license": "ISC",
  "main": "lib/module.js",
  "nyc": {
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 99,
    "include": [
      "lib/**/*.js"
    ],
    "exclude": [
      "test",
      "cli-quizzer.js"
    ],
    "cache": true,
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jamescostian/flashcardz.git"
  },
  "scripts": {
    "cov": "nyc --reporter=html --reporter=text node test/start.js",
    "coveralls": "nyc --reporter=text-lcov node test/start.js | coveralls",
    "pretest": "snazzy",
    "test": "node test/start.js | tap-spec"
  }
}
